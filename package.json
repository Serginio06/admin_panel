{
  "name": "treasure",
  "version": "0.1.0",
  "scripts": {
    "dbUp": "db-migrate up --config ./config/database.json -e dev",
    "eslint": "eslint ./source/**/*.js* --fix",
    "tslint": "tslint ./source/**/*.ts* --fix",
    "clean": "rimraf ./build",
    "buildClient": "webpack --progress -p --config webpack.prod.config.js",
    "buildServer": "tsc && npm run copy",
    "buildServer:watch": "tsc -w && npm run copy",
    "copy": "copyfiles -f ./source/client/resources/icons/favicon* ./build/media/ && cd ./source/server/src && copyfiles ./templates/* ../../../build/server/src && copyfiles ./views/**/* ../../../build/server/src && copyfiles ./views/* ../../../build/server/src",
    "build": "npm run clean && concurrently \"npm run buildClient\" \"npm run buildServer\"",
    "server": "node ./build/server/src/app.js",
    "nodemonServer": "nodemon ./source/server/src/app.js --exec babel-node --presets env,react --plugins transform-object-rest-spread --watch source/server",
    "start:old": "npm run clean; npm run watch; npm run buildServer; npm run server",
    "start": "concurrently --kill-others \"npm run start:client\" \"npm run start:server:watch\"",
    "start:server": "npm run buildServer && npm run nodemonServer",
    "server:demon": "nodemon --watch ./build/server ./build/server/src/app.js",
    "start:watch": "concurrently --kill-others \"npm run start:client\" \"npm run start:server:watch\"",
    "start:server:watch": "concurrently --kill-others \"npm run buildServer:watch\" \"npm run server:demon\"",
    "start:client": "webpack --watch --progress",
    "dev": "npm run clean && npm run watch && npm run nodemonServer",
    "test": "testcafe chrome source/client/test/**/*.test.ts",
    "test:headless": "testcafe chrome:headless source/client/test/**/*.test.ts",
    "test:debug": "testcafe --inspect-brk chrome source/client/test/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://michael_lozinsky@bitbucket.org/Serhii_Ivanchenko/treasure.git"
  },
  "author": "Serhii Ivanchecnko, Michael Lozinsky",
  "license": "ISC",
  "homepage": "https://bitbucket.org/Serhii_Ivanchenko/treasure#readme",
  "babel": {
    "presets": [
      "es2015",
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
